{
    "name": "rentease/api",
    "description": "RentEase API",
    "type": "project",
    "require": {
        "php": ">=7.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "guzzlehttp/guzzle": "^7.0"
    },
    "autoload": {
        "classmap": [
            "controllers/",
            "models/",
            "services/",
            "utils/",
            "middleware/",
            "config/"
        ]
    },
    "autoload-dev": {
        "classmap": [
            "tests/"
        ]
    },
    "scripts": {
        "test": "phpunit",
        "test-unit": "phpunit --testsuite Unit",
        "test-integration": "phpunit --testsuite Integration"
    }
}
